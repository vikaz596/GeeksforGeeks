import java.util.Scanner;

public class GFG {
	
	public static void main(String[] args) {
		Scanner stdin = new Scanner(System.in);
		int testCases = stdin.nextInt();
		for(int t=0 ; t<testCases ; t++) {
			int n = stdin.nextInt();
			
			int arr[] = new int[n];
			
			int max=0;
			for(int i=0 ; i<n ; i++) {
				arr[i] = stdin.nextInt();
				if(arr[i]>max)
					max = arr[i];
			}
			
			System.out.println(max);
		}	
		stdin.close();
	}
}
