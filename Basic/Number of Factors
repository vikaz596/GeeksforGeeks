import java.util.Scanner;

class GFG {
	public static void main (String[] args) {
		Scanner stdin = new Scanner(System.in);
		int testCases = stdin.nextInt();
		for(int t=0 ; t<testCases ; t++) {
			
			int num=stdin.nextInt();
			int count=1;
			
			for(int i=2 ; i<=num ; i++)
				if(num%i==0)
					count++;
				
			System.out.println(count);	
			
		}
		stdin.close();	
	}
}
