import java.util.*;
import java.lang.*;
import java.io.*;

class GFG {
	public static void main (String[] args) {
		Scanner stdin = new Scanner(System.in);
		int testCases = stdin.nextInt();
		for(int t=0 ; t<testCases ; t++) {
			int n = stdin.nextInt();
			
			int sum=0;
			for(int i=0 ; i<n ; i++) 
				sum += stdin.nextInt();
			
			System.out.println(sum);
		}
		stdin.close();	
	}
}
