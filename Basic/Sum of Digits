import java.util.Scanner;

class GFG {
	public static void main (String[] args) {
		Scanner stdin = new Scanner(System.in);
		int testCases = stdin.nextInt();
		for(int t=0 ; t<testCases ; t++) {
			int num = stdin.nextInt();
			int sum=0;
			while(num>0) {
				sum += num%10;
				num = num/10;
			}
			System.out.println(sum);
		}
	}
}
